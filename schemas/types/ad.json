{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"id": {
			"type": "number"
		},
		"name": {
			"type": "string"
		},
		"adType": {
			"type": "string",
			"enum": ["VIDEO", "IMAGE"]
		},
		"filename": {
			"type": "string"
		},
		"duration": {
			"type": "string"
		},
		"order": {
			"type": "number"
		},
		"suborder": {
			"type": "number"
		},
		"sponsorName": {
			"type": "string"
		},
		"type": {
			"type": "string",
			"enum": ["ad"]
		},
		"state": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"canStart": {
					"type": "boolean",
					"default": false
				},
				"started": {
					"type": "boolean",
					"default": false
				},
				"canComplete": {
					"type": "boolean",
					"default": false
				},
				"completed": {
					"type": "boolean",
					"default": false
				},
				"durationFrames": {
					"type": "number",
					"default": 0
				},
				"framesLeft": {
					"type": "number",
					"default": 0
				},
				"frameNumber": {
					"type": "number",
					"default": 0
				},
				"fps": {
					"type": "number",
					"default": 60
				},
				"hasFile": {
					"type": "boolean",
					"default": false
				}
			},
			"required": [
				"canStart",
				"started",
				"canComplete",
				"completed",
				"durationFrames",
				"framesLeft",
				"frameNumber",
				"fps",
				"hasFile"
			]
		}
	},
	"required": [
		"id",
		"name",
		"adType",
		"filename",
		"duration",
		"order",
		"suborder",
		"sponsorName",
		"type",
		"state"
	]
}
